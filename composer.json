{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
        "barryvdh/laravel-cors" : "0.7.*",
        "aws/aws-sdk-php": "3.2.*",
        "aws/aws-sdk-php-laravel": "3.0.*",
        "intervention/image": "2.1.2 ",
        "intervention/imagecache": "2.1.1",
        "Chumper/Zipper": "0.6.x",
        "guzzlehttp/guzzle": "6.0.*",
        "lucadegasperi/oauth2-server-laravel": "4.0.*",
        "league/flysystem-aws-s3-v2": "1.0.1",
        "graham-campbell/throttle": "4.1.*",
        "ramsey/uuid": "~2.8",
        "doctrine/dbal": "~2.3",
		"league/flysystem-aws-s3-v3": "^1.0"

	},
	"require-dev": {
        "fzaninotto/faker": "1.5.*@dev",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"php-vcr/php-vcr": "~1.2",
        "php-vcr/phpunit-testlistener-vcr": "1.1.5"
	},
	"autoload": {
		"classmap": [
			"database",
            "database/seeds",
            "tests/api/ApiTester.php"
        ],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
